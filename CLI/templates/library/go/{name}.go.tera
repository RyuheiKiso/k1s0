package {{ service_name | snake_case }}

// Config はライブラリの設定を表す。
type Config struct {
	Name string
}

// Client は {{ service_name }} ライブラリのクライアントを表す。
type Client struct {
	config Config
}

// New は Client を生成する。
func New(cfg Config) *Client {
	return &Client{config: cfg}
}

// Name は設定名を返す。
func (c *Client) Name() string {
	return c.config.Name
}
