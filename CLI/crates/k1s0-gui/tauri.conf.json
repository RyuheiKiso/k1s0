{
  "productName": "k1s0",
  "version": "0.1.0",
  "identifier": "com.k1s0.gui",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "ui/dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "k1s0",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon-512.png",
      "icons/icon.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
