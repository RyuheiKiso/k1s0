{
  "realm": "k1s0",
  "enabled": true,
  "displayName": "k1s0",
  "sslRequired": "all",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "accessTokenLifespan": 900,
  "accessTokenLifespanForImplicitFlow": 900,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespan": 5184000,
  "refreshTokenMaxReuse": 0,
  "revokeRefreshToken": true,
  "accessCodeLifespan": 60,
  "accessCodeLifespanLogin": 1800,
  "accessCodeLifespanUserAction": 300,
  "actionTokenGeneratedByAdminLifespan": 43200,
  "actionTokenGeneratedByUserLifespan": 300,
  "defaultSignatureAlgorithm": "RS256",
  "roles": {
    "realm": [
      {
        "name": "sys_admin",
        "description": "System administrator with full access to system tier resources",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "sys_operator",
        "description": "System operator with operational access to system tier resources",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "sys_auditor",
        "description": "System auditor with read-only access for audit and compliance",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "user",
        "description": "Default authenticated user role",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "defaultRoles": ["user"],
  "clients": [
    {
      "clientId": "auth-server",
      "name": "AuthServer",
      "enabled": true,
      "protocol": "openid-connect",
      "bearerOnly": true,
      "publicClient": false,
      "directAccessGrantsEnabled": false,
      "standardFlowEnabled": false,
      "serviceAccountsEnabled": true,
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "auth_server_scope"
      ],
      "optionalClientScopes": [
        "offline_access"
      ]
    },
    {
      "clientId": "config-server",
      "name": "ConfigServer",
      "enabled": true,
      "protocol": "openid-connect",
      "bearerOnly": true,
      "publicClient": false,
      "directAccessGrantsEnabled": false,
      "standardFlowEnabled": false,
      "serviceAccountsEnabled": true,
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "config_server_scope"
      ],
      "optionalClientScopes": [
        "offline_access"
      ]
    },
    {
      "clientId": "k1s0-bff",
      "name": "K1s0Bff",
      "enabled": true,
      "protocol": "openid-connect",
      "bearerOnly": false,
      "publicClient": false,
      "directAccessGrantsEnabled": false,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": true,
      "secret": "${KEYCLOAK_CLIENT_SECRET}",
      "redirectUris": [
        "https://k1s0-bff.k1s0-system.svc.cluster.local/*",
        "https://k1s0-bff.k1s0-system/*"
      ],
      "webOrigins": [
        "https://k1s0-bff.k1s0-system.svc.cluster.local",
        "https://k1s0-bff.k1s0-system"
      ],
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "k1s0_bff_scope"
      ],
      "optionalClientScopes": [
        "offline_access"
      ]
    },
    {
      "clientId": "react-spa",
      "name": "ReactSpa",
      "enabled": true,
      "protocol": "openid-connect",
      "bearerOnly": false,
      "publicClient": true,
      "directAccessGrantsEnabled": false,
      "standardFlowEnabled": true,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "https://react-spa.k1s0-system/*",
        "http://localhost:3000/*",
        "http://localhost:5173/*"
      ],
      "webOrigins": [
        "https://react-spa.k1s0-system",
        "http://localhost:3000",
        "http://localhost:5173"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      },
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "react_spa_scope"
      ],
      "optionalClientScopes": [
        "offline_access"
      ]
    }
  ],
  "clientScopes": [
    {
      "name": "auth_server_scope",
      "description": "Scope for auth-server client",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      }
    },
    {
      "name": "config_server_scope",
      "description": "Scope for config-server client",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      }
    },
    {
      "name": "k1s0_bff_scope",
      "description": "Scope for k1s0-bff client",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true"
      }
    },
    {
      "name": "react_spa_scope",
      "description": "Scope for react-spa client",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true"
      }
    }
  ],
  "components": {
    "org.keycloak.keys.KeyProvider": [
      {
        "name": "rsa-generated",
        "providerId": "rsa-generated",
        "config": {
          "priority": ["100"],
          "enabled": ["true"],
          "active": ["true"],
          "algorithm": ["RS256"],
          "keySize": ["2048"],
          "keyUse": ["SIG"]
        }
      }
    ]
  },
  "browserSecurityHeaders": {
    "contentSecurityPolicyReportOnly": "",
    "xContentTypeOptions": "nosniff",
    "xRobotsTag": "none",
    "xFrameOptions": "SAMEORIGIN",
    "contentSecurityPolicy": "frame-src 'self'; frame-ancestors 'self'; object-src 'none';",
    "xXSSProtection": "1; mode=block",
    "strictTransportSecurity": "max-age=31536000; includeSubDomains"
  },
  "smtpServer": {},
  "eventsEnabled": true,
  "eventsListeners": ["jboss-logging"],
  "adminEventsEnabled": true,
  "adminEventsDetailsEnabled": true
}
